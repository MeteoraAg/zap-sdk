{
  "name": "@meteora-ag/zap-sdk",
  "version": "1.0.8",
  "description": "A Typescript SDK for interacting with the Zap program on Meteora.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "author": "Minh Do <minh@raccoons.dev>, Dann Wee <dann@raccoons.dev>",
  "keywords": [
    "meteora-ag",
    "zap",
    "damm-v2",
    "dlmm",
    "jupiter"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist && rm -rf node_modules rm -rf pnpm-lock.yaml"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/bun": "latest",
    "@types/invariant": "^2.2.37",
    "tsup": "^8.4.0",
    "tsx": "^4.20.3",
    "bip39": "^3.1.0",
    "ed25519-hd-key": "^1.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@meteora-ag/cp-amm-sdk": "^1.2.3",
    "@meteora-ag/dlmm": "^1.9.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "bn.js": "^5.2.2",
    "decimal.js": "^10.4.2",
    "invariant": "^2.2.4"
  }
}
