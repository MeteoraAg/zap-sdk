{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["es2015"],
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
